{
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": [0],
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
    "no-param-reassign": [2, {"props": false}]
  }
}
